{
  "name": "razshare/catpaw-mysql",
  "description": "A MYSQL library for catpaw that includes a repository attribute for ease of use.",
  "type": "library",
  "license": "GPL-2.0-only",
  "keywords": [
    "php",
    "cli",
    "server",
    "http",
    "websocket",
    "catpaw",
    "starter",
    "template"
  ],
  "homepage": "https://github.com/tncrazvan/catpaw-starter",
  "scripts": {
    "load": "@composer dump-autoload -o",
    "script": "@php ./vendor/razshare/catpaw/scripts/start.php",
    "start": "@script src/main.php",
    "dev": "@script src/main.php dev 100 && @composer run dev"
  },
  "authors": [
    {
      "name": "Razvan Tanase",
      "email": "tangent.jotey@gmail.com",
      "homepage": "https://github.com/tncrazvan",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "CatPaw\\MYSQL\\": "src/lib/"
    }
  },
  "require": {
    "php": ">=8.0",
    "razshare/catpaw": "^2.0.0",
    "amphp/amp": "^2.6",
    "amphp/file": "^2.0",
    "amphp/mysql": "^2.1"
  },
  "config": {
    "preferred-install": "dist",
    "process-timeout": 0
  }
}
